@{
    ViewData["Title"] = "Painel administrativo";
}




<fieldset>
    <legend> Configurações de endereços </legend>

    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <a role="button"  asp-controller="Municipio" asp-action="Index" class="btn btn-primary"><i class="fa fa-tools "></i> Município</a>
                </div>
                <div class="form-group">
                    <a role="button"  asp-controller="Bairro" asp-action="Index" class="btn btn-primary"><i class="fa fa-plus-square"></i> Bairro</a>
                </div>
            </div>
        </div>
    </div>
</fieldset>

<fieldset>
    <legend> Outras configurações </legend>

    <div class="container">
        <div class="row">
            <div class="col-12 col-md-6 col-lg-6">
                <div class="form-group">
                    <a role="button"  asp-controller="CategoriaSocio" asp-action="Index" class="btn btn-primary"><i class="fa fa-plus-square"></i> Categoria do Sócio</a>
                </div>
                <div class="form-group">
                    <a role="button" asp-controller="Organismo" asp-action="Index" class="btn btn-primary"><i class="fa fa-plus-square"></i> Organismos</a>
                </div>
                <div class="form-group">
                    <a role="button" asp-controller="TipoPagamento" asp-action="Index" class="btn btn-primary"><i class="fa fa-plus-square"></i> Tipo de Pagamento</a>
                </div>
            </div>
        </div>
    </div>
</fieldset>

<!-- /.row -->



